return function(color:string)
	local split = string.split(color,",");
	if split[2] then -- If the user is using a color3
		-- Clamp all of the split values to 0-255
		for i,v in pairs(split) do
			split[i] = math.clamp(tonumber(v) or 0,0,255);
		end;
		return Color3.fromRGB(tonumber(split[1]) or 0,tonumber(split[2]) or 0,tonumber(split[3]) or 0);
	elseif colors[color:lower()] then -- If the user is using a color name
		if type(colors[color:lower()]) == "function" then
			return colors[color:lower()]();
		else
			return colors[color:lower()];
		end
	elseif color:sub(1,9):lower() == 'grayscale' then
		local num = tonumber(color:sub(10));
		if num then
			return Color3.fromRGB(num,num,num);
		else
			return Color3.fromRGB(127,127,127);
		end;
	else -- If that isn't a valid color
		return Color3.fromRGB(0,0,0);
	end;
end